import java.util.*;
public class QuickSort2 {
    public static int partition (int[] a, int L, int R) {
	if (a.length <= 1) 
	    return 0;
	int pivot = (int)(Math.random() * (a.length - 1));

	int c = a[R];
	a[R] = a[pivot];
	a[pivot] = c;
	
	if (a[L] > a[R]) {
	    int d = a[L+1];
	    a[L+1] = a[L];
	    a[L] = d;
	}
	partition(a,L+1,R);
	return L;
    }
    
    public static void main( String[] args) {
	int[] a = new int[] {;
	a = {5,12,76,9,4,15,2,56};
	System.out.println(partition(a,0,a.length-1));
    }
}